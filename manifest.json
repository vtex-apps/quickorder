{
  "vendor": "vtex",
  "name": "quickorder",
  "version": "1.0.0",
  "title": "Quickorder",
  "description": "Allow users to add multiple products to the cart at once",
  "defaultLocale": "pt-BR",
  "builders": {
    "messages": "1.x",
    "store": "0.x",
    "react": "3.x",
    "graphql": "1.x",
    "node": "6.x",
    "docs": "0.x"
  },
  "mustUpdateAt": "2019-04-02",
  "scripts": {
    "postreleasy": "bash lint.sh && vtex publish --verbose"
  },
  "credentialType": "absolute",
  "dependencies": {
    "vtex.checkout": "0.x",
    "vtex.render-runtime": "7.x",
    "vtex.store": "2.x",
    "vtex.styleguide": "9.x",
    "vtex.search": "0.x",
    "vtex.store-graphql": "2.x",
    "vtex.search-graphql": "0.x",
    "vtex.checkout-graphql": "0.x",
    "vtex.apps-graphql": "2.x",
    "vtex.checkout-resources": "0.x",
    "vtex.order-manager": "0.x",
    "vtex.order-items": "0.x",
    "vtex.pixel-manager": "1.x",
    "vtex.store-resources": "0.x",
    "vtex.css-handles": "0.x",
    "vtex.store-icons": "0.x"
  },
  "policies": [
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "graphql-query"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/catalog_system/pvt/sku/stockkeepingunitbyid/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/catalog_system/pub/sku/stockkeepingunitidsbyrefids"
      }
    }
  ],
  "billingOptions": {
    "free": true
  },
  "settingsSchema": {},
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
